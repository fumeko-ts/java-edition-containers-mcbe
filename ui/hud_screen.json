  "tab_list_base_grid": {
    "type": "grid",
    "anchor_from": "$top_middle",
    "anchor_to": "$top_middle",
    "size": [140, "100%c"],
    "grid_item_template": "$tab_grid_item",
    "collection_name": "players_collection",
    "grid_dimension_binding": "#players_grid_dimension",
    "bindings": [ {"binding_name": "#players_grid_dimension"} ]
  },
  "tab_list_base_grid_item": {
    "type": "stack_panel",
    "orientation": "horizontal",
    "size": [140, 12],
    "layer": 3,
    "bindings": [
      {
        "binding_name"           : "#collection_index" ,
        "binding_type"           : "collection"        ,
        "binding_collection_name": "players_collection"
      },
      {
        "binding_type": "view",
        "source_property_name": "(((#collection_index > $base_users) or (#collection_index = $base_users)) and ((#collection_index < $extra_users) or (#collection_index = $extra_users)))",
        "target_property_name": "#visible"
      }
    ],
    "controls": [
      {
        "player_picture": {
          "type": "panel",
          "layer": 11,
          "size": "$10x10",
          "controls": [
            {
              "player_gamer_pic": {
                "type": "image",
                "texture": "#texture",
                "size": "$10x10",
                "texture_file_system": "#texture_source",
                "anchor_from": "$top_left",
                "anchor_to": "$top_left",
                "bindings": [
                  {
                    "binding_name"           : "#texture"          ,
                    "binding_type"           : "collection"        ,
                    "binding_collection_name": "players_collection"
                  },
                  {
                    "binding_name"           : "#texture_source"     ,
                    "binding_name_override"  : "#texture_file_system",
                    "binding_type"           : "collection"          ,
                    "binding_collection_name": "players_collection"
                  },
                  {
                    "binding_name"           : "#gamerpic_visible" ,
                    "binding_type"           : "collection"        ,
                    "binding_collection_name": "players_collection",
                    "binding_name_override"  : "#visible"
                  }
                ]
              }
            },
            {
              "player_local_icon": {
                "type": "image",
                "texture": "textures/ui/lan_icon",
                "size": "$10x10",
                "anchor_from": "$top_left",
                "anchor_to": "$top_left",
                "bindings": [
                  {
                    "binding_name"           : "(not #gamerpic_visible)",
                    "binding_type"           : "collection"             ,
                    "binding_collection_name": "players_collection"     ,
                    "binding_name_override"  : "#visible"
                  }
                ]
              }
            }
          ]
        }
      },
      {
        "player_gamertag": {
          "type": "panel",
          "size": ["fill", 12],
          "controls": [
            {
              "gamertag_text": {
                "type": "label",
                "text": "#gamertag",
                "anchor_from": "$left_middle",
                "anchor_to": "$left_middle",
                "size": ["default", "default"],
                "font_type": "default",
                "offset": [1, 0],
                "layer": 11,
                "bindings": [
                  {
                    "binding_name"           : "#gamertag"         ,
                    "binding_type"           : "collection"        ,
                    "binding_collection_name": "players_collection"
                  }
                ]
              }
            }
          ]
        }
      }
    ]
  },
