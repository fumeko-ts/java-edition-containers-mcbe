  "serverdisplay": {
    "type": "panel",
    "size": ["100% - 2px", "100% - 6px"],
    "anchor_from": "top_right",
    "offset": [-20, 1],
    "anchor_to": "top_right",
    "controls": [
      {
        "world_name": {
          "type": "image",
          "texture": "textures/ui/Black",
          "alpha": 0.0,
          "size": ["100%c", 11],
          "ignored": "$is_on_3p_server",
          "anchor_from": "top_right",
          "anchor_to": "top_right",
          "controls": [
            {
              "label": {
                "type": "label",
                "bindings": [
                  {"binding_name": "#player_list_title"},
                  {
                    "binding_type"        : "view"                         ,
                    "source_property_name": "('ยง7' + (#player_list_title))",
                    "target_property_name": "#info_dat"
                  }
                ],
                "layer": 12,
                "text_alignment": "center",
                "anchor_from": "center",
                "anchor_to": "center",
                "offset": [2, 0],
                "shadow": true,
                "ignored": "$is_on_3p_server",
                "text": "#info_dat"
              }
            }
          ]
        }
      },
      {
        "cubecraft@$hud_world_name": {
          "ignored"        : "(not $is_on_3p_server)"                  ,
          "$server_address": "play.cubecraft.net"  ,
          "anchor_from"    : "top_right"                               ,
          "anchor_to"      : "top_right"                               ,
          "$server_name"   : "(#player_list_title = 'CubeCraft Games')"
        }
      },
      {
        "galaxite@$hud_world_name": {
          "ignored"        : "(not $is_on_3p_server)"           ,
          "$server_address": "play.galaxite.net"   ,
          "anchor_from"    : "top_right"                        ,
          "anchor_to"      : "top_right"                        ,
          "$server_name"   : "(#player_list_title = 'Galaxite')"
        }
      },
      {
        "magma-mc@$hud_world_name": {
          "ignored"        : "(not $is_on_3p_server)"           ,
          "$server_address": "play.magmaMC.net"    ,
          "anchor_from"    : "top_right"                        ,
          "anchor_to"      : "top_right"                        ,
          "$server_name"   : "(#player_list_title = 'Magma MC')"
        }
      },
      {
        "hive@$hud_world_name": {
          "ignored"        : "(not $is_on_3p_server)"                ,
          "$server_address": "geo.hivebedrock.network",
          "anchor_from"    : "top_right"                             ,
          "anchor_to"      : "top_right"                             ,
          "$server_name"   : "(#player_list_title = 'Hive Games')"
        }
      }
    ]
  },
  "world_name_template": {
    "type": "image",
    "texture": "textures/ui/Black",
    "alpha": 0,
    "size": ["100%c", 11],
    "bindings": [
      {"binding_name": "#player_list_title"},
      {
        "binding_type"        : "view"        ,
        "source_property_name": "$server_name",
        "target_property_name": "#visible"
      }
    ],
    "controls": [
      {
        "label": {
          "type": "label",
          "text": "$server_address",
          "layer": 12,
          "text_alignment": "center",
          "anchor_from": "center",
          "anchor_to": "center",
          "offset": [2, 0]
        }
      }
    ]
  },
  "player_count": {
    "type": "panel",
    "size": ["100%", 12],
    "controls": [
      {
        "label": {
          "type": "label",
          "size": ["100% - 9px", 10],
          "text": "#pq",
          "anchor_from": "$right_middle",
          "anchor_to": "$right_middle",
          "bindings": [
            {
              "binding_type"           : "collection_details",
              "binding_collection_name": "players_collection"
            },
            {
              "binding_type"           : "collection"        ,
              "binding_collection_name": "players_collection",
              "binding_name"           : "#collection_index"
            },
            {
              "binding_type"        : "view"                   ,
              "source_property_name": "(#collection_index + 1)",
              "target_property_name": "#text"
            },
            {
              "binding_type"        : "view"                                  ,
              "source_property_name": "('ยงa' + (#text) + 'ยง8ยง7 Users online')",
              "target_property_name": "#pq"
            }
          ]
        }
      }
    ]
  },
  "tab_list": {
