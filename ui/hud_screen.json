                        }
                      ]
                    }
                  },
                  {
                    "tab_list_content": {
                      "type": "image",
                      "anchor_from": "$top_middle",
                      "anchor_to": "$top_middle",
                      "offset": [0, 10],
                      "alpha": 0,
                      "layer": 10,
                      "texture": "textures/ui/white",
                      "size": ["100%c + 8px", "100%c + 6px"],
                      "controls": [
                        {
                          "tab_list_base_grid": {
                            "type": "collection_panel",
                            "size": ["100%c", "100%c"],
                            "collection_name": "players_collection",
