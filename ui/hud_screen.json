          "layer": 50,
          "controls": [
            {"serber_info@$display": {}},
            {
              "logo": {
                "offset": [-200, -113],
                "type": "image",
                "size": [60, 30],
                "layer": 90,
                "texture": "textures/ui/title"
              }
            },
            {
              "player_list_listing": {
                "type": "image",
                "size": ["100%", "100%"],
                "texture": "textures/ui/White",
                "layer": 10,
                "alpha": 0.5,
                "color": [0, 0, 0],
                "anchor_from": "$top_middle",
                "anchor_to": "$top_middle",
                "controls": [
                  {
                    "player_count": {
                      "type": "image",
                      "anchor_from": "$top_middle",
                      "anchor_to": "$top_middle",
