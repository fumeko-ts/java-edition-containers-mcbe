          "max_size": ["100%cm", 216],
          "$base_users": 0,
          "$extra_users": 17,
          "controls": [
            {
              "null@$tab_list_grid": { "offset": [20, 0] }
            }
          ]
        }
      },
      {
        "stack_1": {
          "type": "panel",
          "size": ["100%cm", "100%c"],
          "max_size": ["100%cm", 216],
          "$base_users": 18,
          "$extra_users": 35,
          "controls": [
            {
              "null@$tab_list_grid": { "offset": [0, -216] }
            }
          ],
          "bindings": [
            {
              "binding_name"           : "#collection_index" ,
