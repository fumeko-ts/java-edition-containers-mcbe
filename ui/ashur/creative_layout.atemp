         {"requires": "($is_inventory_tab = true)", "$item_aux": 3538946},
         {"requires": "($is_search_tab = true)", "$item_aux": 27328512},
         {"requires": "($is_construction_tab = true)", "$item_aux": 2949120},
         {"requires": "($is_equipment_tab = true)", "$item_aux": 22740992},
         {"requires": "($is_nature_tab = true)", "$item_aux": 131072},
         {"requires": "($is_items_tab = true)", "$item_aux": 26083328},
         {
            "requires": "($is_left_most_tab = true)",
            "$checked": "assets/minecraft/textures/gui/sprites/container/creative_inventory/tab_top_selected_1",
            "$unchecked": "assets/minecraft/textures/gui/sprites/container/creative_inventory/tab_top_unselected_1"
         },
         {
            "requires": "($is_bottom_right_most_tab = true)",
            "$checked": "assets/minecraft/textures/gui/sprites/container/creative_inventory/tab_bottom_selected_7.png",
            "$unchecked": "assets/minecraft/textures/gui/sprites/container/creative_inventory/tab_bottom_unselected_7.png"
         },
         {
            "requires": "($is_right_most_tab = true)",
            "$checked": "assets/minecraft/textures/gui/sprites/container/creative_inventory/tab_top_selected_7.png",
            "$unchecked": "assets/minecraft/textures/gui/sprites/container/creative_inventory/tab_top_unselected_7.png"
         },
         {
            "requires": "($is_unchecked = true or $is_unchecked_locked = true)",
            "$state_offset": [0, 0]
         },
         {
            "requires": "($is_unchecked_locked_hover = true or $is_unchecked_hover = true)",
            "$state_offset": [0, -0.6],
            "$hover_tab_offset": [0, -0.6]
         },
         {
            "requires": "($is_unchecked_locked_hover = true or $is_unchecked_hover = true and $is_inventory_tab = true)",
            "$state_offset": [0, -1.4],
            "$hover_tab_offset": [0, 0.6]
         },
         {
            "requires": "($is_checked = true or $is_checked_hover = true or $is_checked_locked or $is_checked_locked_hover and $is_inventory_tab = true)",
            "$state_offset": [0, 0]
         },
         {
            "requires": "($is_unchecked = true or $is_unchecked_locked = true and $is_inventory_tab = true)",
            "$state_offset": [0, -2],
            "$hover_tab_offset": [0, -2]
         }
      ],
      "$tab_texture_controlers": [
         {
            "icon": {
               "anchor_from": "center",
               "anchor_to": "center",
               "layer": 16,
               "offset": "$state_offset",
               "size": "$size",
               "type": "panel",
               "controls": [
                  {
                     "item_renderer@beacon.item_renderer": {
                        "property_bag": {"#item_id_aux": "$item_aux"}
                     }
                  }
               ]
            }
         },
         {
            "tab_unchecked": {
               "anchor_from": "center",
               "anchor_to": "center",
               "ignored": "(not $is_unchecked)",
               "layer": 1,
               "offset": [0, 0],
               "size": [26, 36],
               "texture": "$unchecked",
               "type": "image"
            }
         },
         {
            "tab_unchecked_hover": {
               "anchor_from": "center",
               "anchor_to": "center",
               "ignored": "(not $is_unchecked_hover)",
               "layer": 1,
               "offset": "$hover_tab_offset",
               "size": [26, 36],
               "texture": "$unchecked",
               "type": "image"
            }
         },
         {
            "tab_unchecked_locked": {
               "anchor_from": "center",
               "anchor_to": "center",
               "ignored": "(not $is_unchecked_locked)",
               "layer": 1,
               "offset": [0, 0],
               "size": [26, 36],
               "texture": "$unchecked",
               "type": "image"
            }
         },
         {
            "tab_unchecked_locked_hover": {
               "anchor_from": "center",
               "anchor_to": "center",
               "ignored": "(not $is_unchecked_locked_hover)",
               "layer": 1,
               "offset": "$hover_tab_offset",
               "size": [26, 36],
               "texture": "$unchecked",
               "type": "image"
            }
         },
         {
            "tab_checked": {
               "anchor_from": "center",
               "anchor_to": "center",
               "ignored": "(not $is_checked)",
               "layer": 14,
               "offset": [0, -2],
               "size": [26, 36],
               "texture": "$checked",
               "type": "image"
            }
         },
         {
            "tab_checked_hover": {
               "anchor_from": "center",
               "anchor_to": "center",
               "ignored": "(not $is_checked_hover)",
               "layer": 14,
               "offset": [0, -2],
               "size": [26, 36],
               "texture": "$checked",
               "type": "image"
            }
         },
         {
            "tab_checked_locked": {
               "anchor_from": "center",
               "anchor_to": "center",
               "ignored": "(not $is_checked_locked)",
               "layer": 14,
               "offset": [0, -2],
               "size": [26, 36],
               "texture": "$checked",
               "type": "image"
            }
         },
         {
            "tab_checked_locked_hover": {
               "anchor_from": "center",
               "anchor_to": "center",
               "ignored": "(not $is_checked_locked_hover)",
               "layer": 14,
               "offset": [0, -2],
               "size": [26, 36],
               "texture": "$checked",
               "type": "image"
            }
         }
      ]
   },
   "tab": {
