      "$is_search_tab|default": false,
      "$is_construction_tab|default": false,
      "$is_equipment_tab|default": false,
      "$is_items_tab|default": false,
      "$is_nature_tab|default": false,
      "$is_right_most_tab|default": false,
      "$is_left_most_tab|default": false,
      "$is_bottom_right_most_tab|default": false,
      "$not_bottom_tab|default": true,
