{
   "namespace": "furnace_zeta",

   "tab": {
      "type": "panel",
     "anchor_from": "top_left",
      "anchor_to": "top_left",
      "$tab_view_binding_name|default": "tab",
      "$is_checked_state|default": false,
      "controls": [
        {
          "$tab_view_binding_name@common.toggle": {
            "controls": [
               {
                  "unchecked@$tab_texture_definition_grabber": {
                    "$tab_state": "unchecked",
                    "$tab_layer": -3,
                    "$is_unchecked|default": true,
                    "layer": 1
                  }
                },
                {
                  "unchecked_hover@$tab_texture_definition_grabber": {
                    "$tab_state": "unchecked_hover",
                    "$tab_layer": -3,
                    "$is_unchecked_hover|default": true,
                    "$hover": true,
                    "$show_focus": true,
                    "layer": 1
                  }
                },
                {
                  "unchecked_locked@$tab_texture_definition_grabber": {
                    "$tab_state": "unchecked_locked",
                    "$tab_layer": -3,
                    "$is_unchecked_locked|default": true,
                    "layer": 1
                  }
                },
                {
                  "unchecked_locked_hover@$tab_texture_definition_grabber": {
                    "$tab_state": "unchecked_locked_hover",
                    "$tab_layer": -3,
                    "$is_unchecked_locked_hover|default": true,
                    "layer": 1
                  }
                },
                {
                  "checked@$tab_texture_definition_grabber": {
                    "$tab_state": "checked",
                    "$is_checked_state": true,
                    "$is_checked|default": true,
                    "layer": 2
                  }
                },
                {
                  "checked_hover@$tab_texture_definition_grabber": {
                    "$tab_state": "checked_hover",
                    "$is_checked_state": true,
                    "$is_checked_hover|default": true,
                    "$show_focus": true,
                    "$hover": true,
                    "layer": 2
                  }
                },
                {
                  "checked_locked@$tab_texture_definition_grabber": {
                    "$tab_state": "checked_locked",
                    "$is_checked_state": true,
                    "$is_checked_locked|default": true,
                    "layer": 2
                  }
                },
                {
                  "checked_locked_hover@$tab_texture_definition_grabber": {
                    "$tab_state": "checked_locked_hover",
                    "$is_checked_state": true,
                    "$is_checked_locked_hover|default": true,
                    "layer": 2
                  }
                }
            ]
          }
        }
      ]
    },
    "section_tab@tab": {
      "$tab_layer": 2,
      "size": "$tab_size",
      "anchor_from": "top_left",
      "anchor_to": "top_left",
      "$tab_size|default": [ 81, 14 ],
      "$radio_toggle_group": true,
      "$tab_texture_definition_grabber|default": "furnace_zeta.tab_state_common",
      "$image_panel|default": "furnace_zeta.section_tab_image_content_panel",
      "$default_text_color|default": "$button_default_text_color",
      "$hover_text_color|default": "$button_hover_text_color",
      "$locked_text_color|default": "$button_locked_text_color",
      "$toggle_button_mappings|default": [
        {
          "from_button_id": "button.menu_select",
          "to_button_id": "button.menu_select",
          "mapping_type": "pressed"
        },
        {
          "from_button_id": "button.menu_ok",
          "to_button_id": "button.menu_ok",
          "mapping_type": "focused"
        }
      ]
    },
  

    /*  all the tab toggles go below fumeko  */
"creative_tab@furnace_zeta.navigation_tab": {
  "$misc_tab": 1,
  "$burnables_tab": 2, 
  "$smeltable_tab": 3,
  "$toggle_focus_enabled": false,
  "$toggle_name": "navigation_tab",
  "$radio_toggle_group": true,
  "$toggle_group_default_selected": "$misc_tab"
},

"misc_tab@furnace_zeta.creative_tab": {
  "$tab_view_binding_name": "misc_tab_toggle",
  "$toggle_group_forced_index": "$misc_tab",
  "$toggle_binding_type": "global",
  "$toggle_state_binding_name": "#is_left_tab_construct",
  "$focus_id": "misc_tab",
  "$focus_override_left": "FOCUS_OVERRIDE_STOP",
  "$focus_override_right": "FOCUS_OVERRIDE_STOP",
  "$focus_override_down": "burnables_tab"
},

"burnables_tab@furnace_zeta.creative_tab": {
  "$tab_view_binding_name": "burnables_tab_toggle", 
  "$toggle_group_forced_index": "$burnables_tab",
  "$toggle_binding_type": "global",
  "$toggle_state_binding_name": "#is_left_tab_burnables",
  "$focus_id": "burnables_tab",
  "$focus_override_left": "FOCUS_OVERRIDE_STOP",
  "$focus_override_down": "smeltable_tab",
  "$focus_override_up": "misc_tab"
},

"smeltable_tab@furnace_zeta.creative_tab": {
  "$tab_view_binding_name": "smeltable_tab_toggle",
  "$toggle_group_forced_index": "$smeltable_tab",
  "$toggle_binding_type": "global", 
  "$toggle_state_binding_name": "#is_left_tab_smeltable",
  "$focus_id": "smeltable_tab",
  "$focus_override_left": "FOCUS_OVERRIDE_STOP",
  "$focus_override_right": "nature_tab",
  "$focus_override_up": "burnables_tab"
},
    "1@common.empty_panel":{"controls":[{"burn@alpha_tepes_offsets":{}}]},
    "2@common.empty_panel":{"controls":[{"burn@alpha_tepes_offsets":{"$burnables|default": true}}]},
    "3@common.empty_panel":{"controls":[{"burn@alpha_tepes_offsets":{"$smeltables|default": true}}]},

   "navigation_tab@tab": {
      "$tab_texture_definition_grabber|default": "furnace_zeta.tab_state_common"
    },

"all_items_tab@furnace_zeta.1": {
  "bindings": [
    {
      "binding_type": "view",
      "source_control_name": "misc_tab_toggle",
      "source_property_name": "#toggle_state",
      "target_property_name": "#visible"
    }
  ]
},

"fule_tab@furnace_zeta.2": {
  "bindings": [
    {
      "binding_type": "view", 
      "source_control_name": "burnables_tab_toggle",
      "source_property_name": "#toggle_state",
      "target_property_name": "#visible"
    }
  ]
},

"items_tab_content_panel@furnace_zeta.3": {
  "bindings": [
    {
      "binding_type": "view",
      "source_control_name": "smeltable_tab_toggle",
      "source_property_name": "#toggle_state", 
      "target_property_name": "#visible"
    }
  ]
},
