     "tepes_button_checked_hover@furnace_zeta.tepes_button_unchecked": {},
     "tepes_prev_hov@furnace_zeta.tepes_prev_hover": {},
     "tepes_next@furnace_zeta.tepes_next": {},
     "tepes_next_hov@furnace_zeta.tepes_next_hover": {},
