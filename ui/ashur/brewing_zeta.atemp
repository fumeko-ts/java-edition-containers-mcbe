        "texture": "textures/ui/brewing_pipes",
        "anchor_from": "center",
        "anchor_to": "center",
        "offset": [0, 3],
        "alpha": 0,
        "size": [30, 26]
     },
     "brewing_fuel_pipes": {
        "type": "image",
        "layer": 2,
        "texture": "textures/ui/brewing_fuel_pipes",
        "anchor_from": "center",
        "anchor_to": "center",
        "offset": [-42, 0],
        "alpha": 0,
        "size": [30, 22]
     },
