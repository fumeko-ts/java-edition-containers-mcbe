        "alpha": 0,
        "anchor_from": "center",
        "anchor_to": "center"
     },
     "bubbles_full_image": {
        "type": "image",
        "texture": "textures/ui/bubbles_full",
        "layer": 3,
        "offset": [-23, -14],
        "size": [12, 30],
        "clip_direction": "down",
        "bindings": [
           {
              "binding_name"         : "#brewing_bubbles_ratio",
              "binding_name_override": "#clip_ratio"           ,
              "binding_condition"    : "always"
           }
        ]
     },
     "brewing_arrow_empty_image": {
        "type": "image",
        "layer": 1,
        "texture": "textures/ui/brewing_arrow_empty",
        "anchor_from": "center",
        "anchor_to": "center",
        "offset": [17, -13],
        "alpha": 0,
        "size": [9, 28]
     },
     "brewing_arrow_full_image": {
        "type": "image",
        "layer": 3,
        "texture": "textures/ui/brewing_arrow_full",
        "anchor_from": "center",
        "anchor_to": "center",
        "offset": [16.8, -13],
        "size": [8, 29],
        "clip_direction": "up",
        "bindings": [
           {
              "binding_name"         : "#brewing_arrow_ratio",
              "binding_name_override": "#clip_ratio"         ,
              "binding_condition"    : "always"
           }
        ]
     },
     "brewing_label": {
        "type": "label",
        "offset": [0, -3],
        "anchor_from": "top_middle",
        "anchor_to": "top_middle",
        "text": "$container_title",
        "color": "$title_text_color",
        "layer": 2
     },
     "brewingstand_output_item@common.container_item": {
        "size": [18, 18],
        "$cell_overlay_ref": "brewing_zeta.bottle_empty_image",
        "$item_collection_name": "brewing_result_items"
     },
     "brewing_output_slots": {
        "type": "grid",
        "size": [54, 18],
        "offset": [0, 23],
        "grid_dimensions": [3, 1],
        "collection_name": "brewing_result_items",
        "controls": [
           {
              "left_offset": {
                 "type": "panel",
                 "grid_position": [0, 0],
                 "controls": [
                    {
                       "output_grid_item1@brewing_zeta.brewingstand_output_item": {
                          "offset": [-5, -7],
                          "$focus_override_left_binding_type": "none",
                          "$focus_override_left": "brewing_fuel"
                       }
                    }
                 ]
              }
           },
           {
              "output_grid_item2@brewing_zeta.brewingstand_output_item": {
                 "grid_position": [1, 0]
              }
           },
           {
              "right_offset": {
                 "type": "panel",
                 "grid_position": [2, 0],
                 "controls": [
                    {
                       "output_grid_item3@brewing_zeta.brewingstand_output_item": {
                          "offset": [5, -7]
                       }
                    }
                 ]
              }
           }
        ]
     },
     "brewing_input_slot": {
        "type": "panel",
        "size": [17, 22],
        "offset": [0, -18],
        "controls": [
           {
              "input_grid_item@common.container_item": {
                 "$item_collection_name"            : "brewing_input_item",
                 "$focus_override_left_binding_type": "none"              ,
                 "$focus_override_left"             : "brewing_fuel"
              }
           }
        ]
     },
     "brewing_fuel_slot": {
        "type": "panel",
        "size": [18, 18],
        "offset": [-63, -13],
        "controls": [
           {
              "fuel_grid_item@common.container_item": {
                 "$cell_overlay_ref": "brewing_zeta.fuel_empty_image",
                 "$item_collection_name": "brewing_fuel_item",
                 "$focus_id_binding_type": "none",
                 "$focus_id": "brewing_fuel",
                 "$focus_override_right_binding_type": "none",
                 "$focus_override_right": "brewing_input_item0"
              }
           }
        ]
     },
     "brewing_panel_top_half": {
        "type": "panel",
        "size": ["99%", "51% - 13px"],
