   "tepes_button_checked@survival_layout.tepes_button_unchecked": {},
   "tepes_button_checked_hover@survival_layout.tepes_button_unchecked": {},
   "tepes_prev_hov@survival_layout.tepes_prev_hover": {},
   "tepes_next@survival_layout.tepes_next": {},
   "tepes_next_hov@survival_layout.tepes_next_hover": {},
   "base_toggle_button": {
      "type": "panel",
      "anchor_from": "top_left",
      "anchor_to": "top_left",
      "size": [20, 18],
      "$toggle_group|default": true,
      "layer": 4,
      "controls": [
         {
            "$toggle_control_name@common.toggle": {
               "radio_toggle_group": "$toggle_group",
               "$hover_state|default": true,
               "focus_wrap_enabled": true,
               "controls": [
                  { "unchecked@survival_layout.base_toggle_button_state": {"$bg_image": "$unchecked_bg"} }                                ,
                  { "unchecked_hover@survival_layout.base_toggle_button_state": {"$bg_image": "$unchecked_hov_bg", "$hover_state": true} },
                  { "checked@survival_layout.base_toggle_button_state": {"$bg_image": "$checked_bg"} }                                    ,
                  { "checked_hover@survival_layout.base_toggle_button_state": {"$bg_image": "$checked_hov_bg", "$hover_state": true} }
               ]
            }
         }
      ]
   },
