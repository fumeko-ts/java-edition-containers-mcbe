   "tepes_next_button@survival_layout.base_toggle_button": {
      "focus_container": true,
      "use_last_focus": true,
      "ignored": "$ignore_next_button",
      "anchor_from": "top_right",
      "anchor_to": "top_right",
      "layer": 10,
      "$toggle_group_forced_index": 1,
