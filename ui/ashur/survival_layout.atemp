      "$focus_override_left": "nature_tab",
      "$focus_override_up": "FOCUS_OVERRIDE_STOP"
   },
   "equipment_tab@survival_layout.top_tab": {
      "$tab_view_binding_name": "equipment_tab_toggle",
      "$tab_icon": "survival_layout.equipment_icon",
      "$toggle_group_forced_index": "$equipment_index",
      "$toggle_binding_type": "global",
      "$toggle_state_binding_name": "#is_left_tab_equipment",
      "$top_tab_tts_name": "craftingScreen.tab.equipment",
      "$focus_id": "equipment_tab",
      "$focus_override_up": "search_tab",
      "$focus_override_down": "nature_tab",
      "$ingame_t": true,
      "$animate": true,
      "variables": [
         { "requires": "($animate or ($ingame_t = true))", "$offset": [0, 0] }
      ]
   },
   "nature_tab@survival_layout.top_tab": {
      "$tab_view_binding_name": "nature_tab_toggle",
      "$tab_icon": "survival_layout.nature_icon",
      "$toggle_group_forced_index": "$nature_index",
      "$toggle_binding_type": "global",
      "$toggle_state_binding_name": "#is_left_tab_nature",
      "$top_tab_tts_name": "craftingScreen.tab.nature",
      "$focus_id": "nature_tab",
      "$focus_override_up": "equipment_tab",
      "$focus_override_down": "items_tab",
      "$ingame_t": true,
      "$animate": true,
      "variables": [
         { "requires": "($animate or ($ingame_t = true))", "$offset": [0, 0] }
      ]
   },
   "items_tab@survival_layout.top_tab": {
      "$tab_view_binding_name": "items_tab_toggle",
      "$tab_icon": "survival_layout.miscellaneous_icon",
      "$toggle_group_forced_index": "$items_index",
      "$toggle_binding_type": "global",
      "$toggle_state_binding_name": "#is_left_tab_items",
      "$top_tab_tts_name": "craftingScreen.tab.items",
      "$focus_id": "items_tab",
      "$focus_override_up": "nature_Tab",
      "$focus_override_down": "FOCUS_OVERRIDE_STOP",
      "$ingame_t": true,
      "$animate": true,
      "variables": [
         { "requires": "($animate or ($ingame_t = true))", "$offset": [0, 0] }
      ]
   },
   "warning_experimental": { "type": "image", "texture": "textures/warning_experimental", "size": [1158, 79] },
   "construction_tab@survival_layout.top_tab": {
      "$is_left_most_tab": true,
      "$tab_view_binding_name": "construction_tab_toggle",
      "$tab_icon": "survival_layout.construction_icon",
      "$toggle_group_forced_index": "$construction_index",
      "$toggle_binding_type": "global",
      "$toggle_state_binding_name": "#is_left_tab_construct",
      "$top_tab_tts_name": "craftingScreen.tab.construction",
      "$focus_id": "construction_tab",
      "$focus_override_left": "help_button",
      "$focus_override_right": "equipment_tab",
      "$focus_override_up": "FOCUS_OVERRIDE_STOP",
      "$unchecked_default_image": "assets/minecraft/textures/gui/sprites/recipe_book/tab",
      "$unchecked_hover_image": "assets/minecraft/textures/gui/sprites/recipe_book/tab",
      "$unchecked_locked_image": "assets/minecraft/textures/gui/sprites/recipe_book/tab",
      "$unchecked_locked_hover_image": "assets/minecraft/textures/gui/sprites/recipe_book/tab",
      "$checked_default_image": "assets/minecraft/textures/gui/sprites/recipe_book/tab_selected",
      "$checked_hover_image": "assets/minecraft/textures/gui/sprites/recipe_book/tab_selected",
      "$checked_locked_image": "assets/minecraft/textures/gui/sprites/recipe_book/tab_selected",
      "$checked_locked_hover_image": "assets/minecraft/textures/gui/sprites/recipe_book/tab_selected",
      "$ingame_t": true,
      "$animate": true,
      "variables": [
         { "requires": "($animate or ($ingame_t = true))", "$offset": [0, 0] }
      ]
   },
   "text_edit_control@common.text_edit_box_beta": {
      "size": [81, 14],
      "property_bag": {"#property_field": "#item_name"},
      "$shadow": true,
      "max_length": 30,
      "$text_edit_binding_name": "#text_box_item_name",
      "$text_edit_box_label_size": ["default", 8],
      "$place_holder_text": "§o§8§7Search...",
      "$text_edit_box_label_offset": [1, -1],
      "$text_edit_box_hovered_button_id": "button.search_bar_hovered",
      "$text_edit_box_clear_to_button_id": "button.search_bar_clear",
      "$text_edit_box_selected_to_button_id": "button.search_bar_selected",
      "$text_edit_box_deselected_to_button_id": "button.search_bar_deselected",
      "$text_clear_button_enabled": true,
      "$text_clear_button_size": [15, 15],
      "$text_clear_button_offset": [2, 0],
      "focus_wrap_enabled": false,
      "$focus_id": "recipe_search_bar",
      "$focus_override_right": "craftable_toggle",
      "$focus_override_up": ""
   },
   "tepes_previos_button@survival_layout.base_toggle_button": {
      "focus_container": true,
      "use_last_focus": true,
      "ignored": "$ignore_prev_button",
      "anchor_from": "top_right",
      "anchor_to": "top_right",
