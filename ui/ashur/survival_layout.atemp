   "recipe_button_highlighted": { "type": "image", "texture": "assets/minecraft/textures/gui/sprites/recipe_book/button_highlighted", "size": [20, 20] },
   "tepes_button_unchecked": { "type": "image", "texture": "assets/minecraft/textures/gui/sprites/recipe_book/button", "size": [20, 18], "uv_size": [20, 18], "uv": [0, 0], "layer": 2 },
   "tepes_prev": { "type": "image", "texture": "assets/minecraft/textures/gui/sprites/recipe_book/page_backward", "size": [12, 17], "layer": 2 },
   "tepes_prev_hover": { "type": "image", "texture": "assets/minecraft/textures/gui/sprites/recipe_book/page_backward_highlighted", "size": [12, 17], "layer": 2 },
   "tepes_next": { "type": "image", "texture": "assets/minecraft/textures/gui/sprites/recipe_book/page_forward", "size": [12, 17], "layer": 2 },
   "tepes_next_hover": { "type": "image", "texture": "assets/minecraft/textures/gui/sprites/recipe_book/page_forward_highlighted", "size": [12, 17], "layer": 2 },
   "armor_overlay_helmet@crafting.armor_overlay": {"alpha": 0.6, "texture": "textures/ui/empty_armor_slot_helmet"},
   "armor_overlay_chest@crafting.armor_overlay": {"alpha": 0.6, "texture": "textures/ui/empty_armor_slot_chestplate"},
   "armor_overlay_legs@crafting.armor_overlay": {"alpha": 0.6, "texture": "textures/ui/empty_armor_slot_leggings"},
   "armor_image_feet@crafting.armor_overlay": {"alpha": 0.6, "texture": "textures/ui/empty_armor_slot_boots"},
   "armor_overlay_shield@crafting.offhand_overlay": {"alpha": 0.6, "texture": "textures/ui/empty_armor_slot_shield"},
   "icon_image@somali_to_Mori_no_Kamisama.image_element": { "layer": -1, "size": [17, 17] },
   "equipment_icon@survival_layout.icon_image": { "size": [25, 16], "texture": "textures/atemp/ui/icon_recipe_equipment" },
   "construction_icon@survival_layout.icon_image": { "size": [25, 16], "texture": "textures/atemp/ui/icon_recipe_construction" },
   "nature_icon@survival_layout.icon_image": { "size": [28, 19], "texture": "textures/atemp/ui/icon_recipe_nature" },
   "search_icon@survival_layout.icon_image": { "size": [25, 16], "texture": "textures/items/compass_item" },
   "miscellaneous_icon@survival_layout.icon_image": { "size": [16, 16], "texture": "textures/items/redstone_dust" },
   "layout_template_toggle@common_toggles.light_template_toggle": {
      "$focus_override_up"       : "FOCUS_OVERRIDE_STOP"                                                 ,
      "$default_texture"         : "assets/minecraft/textures/gui/sprites/recipe_book/button"            ,
      "$hover_texture"           : "assets/minecraft/textures/gui/sprites/recipe_book/button_highlighted",
      "$pressed_texture"         : "assets/minecraft/textures/gui/sprites/recipe_book/button_highlighted",
      "$pressed_no_hover_texture": "assets/minecraft/textures/gui/sprites/recipe_book/button"
   },
   "recipe_dat": {
      "type": "grid",
      "size": [125, "default"],
      "anchor_to": "top_left",
      "anchor_from": "top_left",
      "$binding_condition|default": "visible",
      "$collection_name": "recipe_book",
      "$grid_item_precache_count|default": 0,
      "$grid_item_template|default": "survival_layout.Delta_grid_item",
      "collection_name": "$collection_name",
      "grid_rescaling_type": "horizontal",
      "grid_item_template": "$grid_item_template",
      "bindings": [
         {
            "binding_name"           : "#recipe_book_total_items",
            "binding_name_override"  : "#maximum_grid_items"     ,
            "binding_condition"      : "$binding_condition"      ,
            "binding_type"           : "collection"              ,
            "binding_collection_name": "$collection_name"
         }
      ]
   },
   "cell_image": {"type": "image", "texture": "textures/ui/cell_image"},
   "recipe_container_cell_images": {
      "type": "panel",
      "size": [25, 25],
      "controls": [
         {
            "recipe_book_item_bg@survival_layout.slot_craftable": {
               "bindings": [
                  {
                     "binding_name"           : "(#container_item_background_texture = 'textures/ui/recipe_book_item_bg')",
                     "binding_name_override"  : "#visible"                                                                ,
                     "binding_type"           : "collection"                                                              ,
                     "binding_collection_name": "$item_collection_name"
                  }
               ]
            }
         },
         {
            "recipe_book_item_group@survival_layout.slot_craftable": {
               "bindings": [
                  {
                     "binding_name"           : "(#container_item_background_texture = 'textures/ui/recipe_book_group_bg' or 'textures/ui/recipe_book_group_expanded' or 'textures/ui/recipe_book_group_collapsed')",
                     "binding_name_override"  : "#visible"                                                                ,
                     "binding_type"           : "collection"                                                              ,
                     "binding_collection_name": "$item_collection_name"
                  }
               ]
            }
         },
         {
            "recipe_book_red_button@survival_layout.slot_uncraftable": {
               "bindings": [
                  {
                     "binding_name"           : "(#container_item_background_texture = 'textures/ui/recipe_book_red_button')",
                     "binding_name_override"  : "#visible"                                                                   ,
                     "binding_type"           : "collection"                                                                 ,
                     "binding_collection_name": "$item_collection_name"
                  }
               ]
            }
         },
         {
            "selected": {
               "type": "panel",
               "bindings": [
                  {
                     "binding_name"           : "(#container_item_background_texture = 'textures/ui/recipe_book_touch_cell_selected')",
                     "binding_name_override"  : "#visible"                                                                            ,
                     "binding_type"           : "collection"                                                                          ,
                     "binding_collection_name": "$item_collection_name"
                  }
               ],
               "controls": [
                  {
                     "recipe_book_touch_cell_selected@survival_layout.slot_craftable": {
                        "property_bag": {"#collection_name": "crafting_output_items", "#collection_index": 0},
                        "bindings": [
                           {
                              "binding_name"           : "(not (#container_item_background = 3))",
                              "binding_name_override"  : "#visible"                              ,
                              "binding_type"           : "collection"                            ,
                              "binding_collection_name": "crafting_output_items"                 ,
                              "binding_condition"      : "always"
                           }
                        ]
                     }
                  },
                  {
                     "recipe_book_red_button@survival_layout.slot_uncraftable": {
                        "property_bag": {"#collection_name": "crafting_output_items", "#collection_index": 0},
                        "bindings": [
                           {
                              "binding_name"           : "(#container_item_background = 3)",
                              "binding_name_override"  : "#visible"                        ,
                              "binding_type"           : "collection"                      ,
                              "binding_collection_name": "crafting_output_items"           ,
                              "binding_condition"      : "always"
                           }
                        ]
                     }
                  }
               ]
            }
         }
      ]
   },
   "slot_craftable@survival_layout.cell_image": {
      "texture": "assets/minecraft/textures/gui/sprites/recipe_book/slot_craftable",
      "$cell_image_size|default": ["default", "default"],
      "size": "$cell_image_size"
   },
   "slot_uncraftable@survival_layout.cell_image": {
      "texture": "assets/minecraft/textures/gui/sprites/recipe_book/slot_uncraftable",
      "$cell_image_size|default": ["default", "default"],
      "size": "$cell_image_size"
   },

    "filter_disabled@common.empty_image":{
      "texture":"assets/minecraft/textures/gui/sprites/recipe_book/filter_disabled.png"
   },
    "filter_disabled_highlighted@common.empty_image":{
      "controls":[{"hover@common.hover_text":{"property_bag":{"#hover_text": "Showing All"}}}],
      "texture":"assets/minecraft/textures/gui/sprites/recipe_book/filter_disabled_highlighted.png"
   },
    "filter_enabled@common.empty_image":{
      "texture":"assets/minecraft/textures/gui/sprites/recipe_book/filter_enabled.png"
   },
    "filter_enabled_highlighted@common.empty_image":{
      "controls":[{"hover@common.hover_text":{"property_bag":{"#hover_text": "Showing Craftable"}}}],
      "texture":"assets/minecraft/textures/gui/sprites/recipe_book/filter_enabled_highlighted.png"
   },

   "filter_toggle@common_toggles.switch_toggle": {
      "size": [26, 18],
      "$unchecked_control": "survival_layout.filter_disabled",
      "$unchecked_hover_control": "survival_layout.filter_disabled_highlighted",
      "$checked_control": "survival_layout.filter_enabled",
      "$checked_hover_control": "survival_layout.filter_enabled_highlighted",
      "$toggle_name": "toggle.enableFiltering",
      "$toggle_binding_type": "global",
      "$toggle_state_binding_name": "#filtering_enabled",
      "$button_mappings": [
         {"to_button_id": "button.filter_toggle_hovered", "mapping_type": "pressed"},
         {"from_button_id": "button.menu_select", "to_button_id": "button.menu_select", "mapping_type": "pressed"},
         {"from_button_id": "button.menu_ok", "to_button_id": "button.menu_ok", "mapping_type": "focused"},
         {"from_button_id": "button.controller_back", "to_button_id": "button.controller_back", "mapping_type": "focused", "ignored": "(not $is_ps4)"},
         {
            "ignored"             : "(not $enable_directional_toggling)",
            "from_button_id"      : "button.menu_left"                  ,
            "to_button_id"        : "$toggle_off_button"                ,
            "mapping_type"        : "pressed"                           ,
            "input_mode_condition": "gamepad_and_not_gaze"
         },
         {
            "ignored"             : "(not $enable_directional_toggling)",
            "from_button_id"      : "button.menu_right"                 ,
            "to_button_id"        : "$toggle_on_button"                 ,
            "mapping_type"        : "pressed"                           ,
            "input_mode_condition": "gamepad_and_not_gaze"
         }
      ]
   },
   "base_offset_label": {"type": "label", "color": "white", "text": "$tepes_offset_label", "anchor_from": "center", "anchor_to": "center", "layer": 40},
   "crafting_label": {"type": "label", "anchor_from": "top_left", "anchor_to": "top_left", "text": "", "color": "$title_text_color", "layer": 2},
   "top_tab@recipe_tabs.tab_left": {
      "anchor_to": "center",
      "anchor_from": "center",
      "$anims|default": [],
      "anims": "$anims",
      "$offset|default": [0, 0],
      "offset": "$offset",
      "$toggle_focus_enabled": false,
      "$toggle_name": "navigation_tab",
      "$toggle_group_default_selected": "$construction_index",
      "$tab_content": "crafting.empty_tab_panel",
      "$allow_controller_back_button_mapping": "$is_ps4"
   },
   "search_tab@survival_layout.top_tab": {
      "$is_right_most_tab": true,
      "$tab_view_binding_name": "search_tab_toggle",
      "$tab_icon": "survival_layout.search_icon",
      "$toggle_group_forced_index": "$search_index",
      "$toggle_binding_type": "global",
      "$toggle_state_binding_name": "#is_left_tab_search",
